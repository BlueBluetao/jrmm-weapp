<view class='container'>
<van-tabs swipeable>
    <van-tab
      wx:for="{{ tabs }}"
      wx:key="index"
      title="{{ item.title }}"
    >
      <view class="content">
        {{ '内容' + item.title }}
      </view>
    </van-tab>
</van-tabs>
<van-tabbar active="{{ activeTarbar }}" bind:change="onChange">
  <van-tabbar-item icon="search">金牌苗圃</van-tabbar-item>
  <van-tabbar-item icon="search">经纪人</van-tabbar-item>
  <van-tabbar-item icon="setting-o">社区</van-tabbar-item>
</van-tabbar>
</view>